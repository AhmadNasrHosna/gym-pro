main.sie-content
  .profile
    header.profile__header
      .profile__header__inner
        .profile__cover
          img(src="assets/images/prof-pic.jpeg", alt="Profile cover")
        .container
          a(href="#0", class="profile__edit-profile") Edit Profile
          .profile__header__left
            ul.list.list--inline.list--no-bullets.profile__social
              li
                a(href="#", class="fb")
                  svg(width='40', height='40', viewBox='0 0 24 24')
                    use(xlink:href='#facebook')
                  span.visually-hidden Facebook
              li
                a(href="#", class="tw")
                  svg(width='40', height='40')
                    use(xlink:href='#icon-twitter')
                  span.visually-hidden Twitter
              li
                a(href="#", class="ins")
                  svg(width='40', height='40', viewBox='0 0 24 24')
                    use(xlink:href='#instagram')
                  span.visually-hidden Instagram
          .profile__header__middle
            .profile__picture
              img(src="assets/images/user-avatar.jpg", alt="User Avatar")
            .profile__username 
              h3 Ahmed Nasr 
          .profile__header__right
            .profile__followers 
              span.followers-count 23
              |  Followers
            button.profile__follow-button.button.button--secondary.button--color-5
              svg(width='40', height='40', viewBox='0 0 72 72')
                use(xlink:href='#icon-plus')
              | Follow
    
