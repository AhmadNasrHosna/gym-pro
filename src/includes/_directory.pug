main.site-content
  .directory
    header.directory__header 
      .directory__header__wrapper
        .directory__form
          .container.container--narrow
            h2.align-center Refine your search
            form(action="")
              .form-control
                select.select-css(name="city")
                  option(selected disabled) City
                  option(value="cairo") Cairo
                  option(value="tanta") Tanta
                  option(value="alexandria") Alexandria   
              .form-control
                select.select-css(name="district")
                  option(selected disabled) District
                  option(value="cairo") Cairo
                  option(value="tanta") Tanta
                  option(value="alexandria") Alexandria   
              .form-control
                select.select-css(name="sorting")
                  option(selected disabled) Sorting
                  option(value="trainer") Trainer
                  option(value="gym") GYM
              input(type="submit", value="Search", class="button button--secondary button--color-5")
      
